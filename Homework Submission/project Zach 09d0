import turtle as t
import math
from random import randint

t.speed(0)
r = randint(0, 255)
g = randint(0, 255)
b = randint(0, 255)
t.color(r/255, g/255, b/255)
t.fillcolor()
for _ in range(100): 
    t.begin_fill()
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.left(65)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.penup()
    t.pendown()
    t.backward(8937)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.left(65)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.penup()
    t.pendown()
    t.circle(60)
    t.goto(0,0)
    t.end_fill()
    t.begin_fill()
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.left(65)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.penup()
    t.pendown()
    t.backward(8937)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.left(65)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.penup()
    t.pendown()
    t.circle(60)
    t.goto(0,0)
    t.end_fill()
    t.begin_fill()
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.left(65)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.penup()
    t.pendown()
    t.backward(8937)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.left(65)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.penup()
    t.pendown()
    t.circle(60)
    t.goto(0,0)
    t.end_fill()
    t.begin_fill()
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.left(65)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.penup()
    t.pendown()
    t.backward(8937)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.left(65)
    t.forward(100)
    t.right(-70)
    t.forward(100)
    t.left(45)
    t.forward(100)
    t.penup()
    t.pendown()
    t.circle(60)
    t.goto(0,0)
    t.end_fill()
    t.penup()
    x = randint(-250, 250)
    y = randint(-250, 250)
    t.penup()
    t.goto(x, y)

turtle.done()
